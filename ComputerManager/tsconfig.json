{
  "compilerOptions": {
    "noImplicitAny": false,
    "baseUrl": ".",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "lib": [ "dom", "es6", "es2015", "scripthost" ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
